%h1 Editing Carer Application
.edit_signup
  - form_for(@signup, :url => admin_signup_interview_path(@signup)) do |f|
    -#= f.error_messages
    = render :partial => 'admin/signups/signup', :locals => {:f => f}
    = render :partial => 'admin/interviews/interview', :locals => {:f => f}
    = f.submit "Update"