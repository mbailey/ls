<h1>Signup: <%=h @signup.first_name.capitalize %> <%=h @signup.last_name.capitalize %></h1>
<dl>
  <% Signup.viewable_fields.map do |f| %>
		<dt><%= f.humanize.capitalize %>:</dt>
		<dd><%=h @signup.send(f) %></dd>
  <% end %>
</dl>

<%= link_to 'Edit', edit_admin_signup_path(@signup) %> |
<%= link_to 'Back', admin_signups_path %>